processes:
  - name: godojo
    executable: /var/vcap/jobs/godojo/bin/godojo.sh
    persistent_disk: true
    ephemeral_disk: true
    additional_volumes:
    - path: /var/vcap/sys/tmp/godojo
      writable: true
      allow_executions: true
    - path: /var/vcap/jobs/godojo/config
      writable: true
    - path: /var/vcap/packages/godojo/bin/django-DefectDojo
      writable: true
    capabilities:
    - SYS_RESOURCE
