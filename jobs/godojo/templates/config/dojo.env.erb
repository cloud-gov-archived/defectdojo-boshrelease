#!/bin/bash

export DD_CSRF_TRUSTED_ORIGINS=<%= p('dojo.csrf.trusted_origins') %>
export DD_SOCIAL_AUTH_SHOW_LOGIN_FORM=False
export DD_SOCIAL_LOGIN_AUTO_REDIRECT=True
export DD_SOCIAL_AUTH_KEYCLOAK_OAUTH2_ENABLED=True
export DD_SOCIAL_AUTH_KEYCLOAK_KEY=<%= p('dojo.auth.client') %>
export DD_SOCIAL_AUTH_KEYCLOAK_SECRET=<%= p('dojo.auth.secret') %>
export DD_SOCIAL_AUTH_KEYCLOAK_AUTHORIZATION_URL=<%= p('dojo.auth.domain') %>/oauth/authorize
export DD_SOCIAL_AUTH_KEYCLOAK_ACCESS_TOKEN_URL=<%= p('dojo.auth.domain') %>/oauth/token
export DD_SOCIAL_AUTH_KEYCLOAK_LOGIN_BUTTON_TEXT="Login"
