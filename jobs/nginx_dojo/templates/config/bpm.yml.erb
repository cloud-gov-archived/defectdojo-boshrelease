processes:
  - name: nginx_dojo
    executable: /var/vcap/jobs/nginx_dojo/bin/nginx.sh
    persistent_disk: true
    ephemeral_disk: true
    additional_volumes:
    - path: /var/vcap/sys/tmp/nginx_dojo
      writable: true
      allow_executions: true
    - path: /var/vcap/sys/tmp/client_body_temp
      writable: true
    - path: /var/vcap/sys/tmp/fastcgi_temp
      writable: true
    - path: /var/vcap/sys/tmp/proxy_temp
      writable: true
    - path: /var/vcap/sys/tmp/scgi_temp
      writable: true
    - path: /var/vcap/sys/tmp/uwsgi_temp
      writable: true
    - path: /var/vcap/jobs/nginx_dojo/config
      writable: true
    - path: /var/vcap/jobs/godojo/django-DefectDojo
      writable: true
    capabilities:
    - SYS_RESOURCE
